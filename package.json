{
  "name": "n8n-nodes-tibber",
  "version": "1.0.6",
  "description": "n8n node to access Tibber API data",
  "keywords": [
    "n8n",
    "node",
    "tibber",
    "energy",
    "smart home",
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "author": {
    "name": "JÃ¼rgen Schilling",
    "email": "alschimmi@googlemail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schimmmi/n8n-nodes-tibber.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "dev": "npx tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts --fix .",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "echo \"Add tests here\" && exit 0",
    "publish:test": "npm publish --dry-run",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TibberApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Tibber/Tibber.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/luxon": "^3.7.1",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "graphql-request": "^7.2.0"
  }
